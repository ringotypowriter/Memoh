{
  "name": "@memoh/cli",
  "version": "0.1.0-beta.3",
  "description": "Command line interface and core API for Memoh",
  "exports": {
    ".": "./src/index.ts",
    "./core": "./src/core/index.ts",
    "./types": "./src/types/index.ts",
    "./utils": "./src/utils/index.ts",
    "./cli": "./src/cli/index.ts"
  },
  "bin": {
    "memoh": "./dist/cli.js"
  },
  "scripts": {
    "start": "bun run src/cli/index.ts",
    "dev": "bun run --watch src/cli/index.ts",
    "build": "tsup",
    "build:watch": "tsup --watch"
  },
  "dependencies": {
    "@memoh/sdk": "workspace:*",
    "commander": "^12.1.0",
    "chalk": "^5.4.1",
    "ora": "^8.1.1",
    "inquirer": "^12.3.0",
    "table": "^6.8.2"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "bun-types": "latest",
    "@types/bun": "latest",
    "tsup": "^8.4.0"
  },
  "packageManager": "pnpm@10.27.0",
  "module": "src/index.ts",
  "type": "module",
  "private": true,
  "peerDependencies": {
    "typescript": "^5"
  }
}
